{
  "name": "build-js-dev-env",
  "version": "1.0.0",
  "description": "JavaScript Development Environment Pluralsight course by Cory House",
  "scripts": {
    "prestart": "babel-node build/startMessage.js",
    "start": "npm-run-all --parallel security:check open:src lint:watch test:watch",
    "open:src": "babel-node build/srcServer.js",
    "lint": "esw webpack.config.* src build --color",
    "lint:watch": "npm run lint -- --watch",
    "security:check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain itkoren",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha --reporter progress build/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "generate:mockdata": "babel-node build/generateMockData",
    "prestart:mockapi": "npm run generate:mockdata",
    "start:mockapi": "json-server --watch src/api/db.json --port 3001",
    "start:all": "npm-run-all --parallel start start:mockapi"
  },
  "author": "Cory House",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "latest"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-env": "latest",
    "babel-preset-latest": "latest",
    "babel-register": "latest",
    "chai": "latest",
    "chalk": "latest",
    "cheerio": "latest",
    "compression": "latest",
    "cross-env": "latest",
    "css-loader": "latest",
    "eslint": "latest",
    "eslint-plugin-import": "latest",
    "eslint-watch": "latest",
    "express": "latest",
    "extract-text-webpack-plugin": "latest",
    "html-webpack-plugin": "latest",
    "jsdom": "latest",
    "json-schema-faker": "latest",
    "json-server": "latest",
    "localtunnel": "latest",
    "mocha": "latest",
    "nock": "latest",
    "npm-run-all": "latest",
    "nsp": "latest",
    "numeral": "latest",
    "open": "latest",
    "rimraf": "latest",
    "style-loader": "latest",
    "webpack": "latest",
    "webpack-dev-middleware": "latest",
    "webpack-hot-middleware": "latest",
    "webpack-md5-hash": "latest"
  }
}
